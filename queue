#include <bits/stdc++.h>
using namespace std;

int main() {
	queue<int> a;
	a.push(10);
	a.push(20);
	a.push(30);
	a.pop();
	cout<<a.front();
}
//priority queue
in python heapify
maximum heap minimum heap
it arranges descending order in a container
priority_queue<int> pq;
pq.push(10);
pq.push(20);
pq.push(30);
# max heap
#include <bits/stdc++.h>
using namespace std;

int main() {
	priority_queue<int>pq;
	pq.push(10);
	pq.push(20);
	pq.push(30);
	pq.pop();
	cout<<pq.top();
}
#min heap
#include <bits/stdc++.h>
using namespace std;

int main() {
	priority_queue<int>pq;
	pq.push(-1*10);
	pq.push(-1*20);
	pq.push(-1*30);
	pq.pop();
	cout<<pq.top()*-1;
}
Minimum heap syntax
priority_queue<int,vector<int>,greater<int>>;
